
<html>
    
    <head>
        <title>Login Here</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    </head>
    <body class="container">
        <div class="page-header">
            <h1>Loure Homes</h1>
        </div>

        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container-fluid">
              <i class="material-icons">camera</i>
              <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="nav navbar-nav">
                    <li><a href="/">Home</a></li>
                    <li><a href="/signup">Sign Up</a></li>
                    <li><a href="/profile">Dashboard</a></li>
                </ul>

                <ul class="nav navbar-nav navbar-right">
                    <li><a href="/login">Log In</a></li>
                    <li><a href="/logout">Log Out</a></li>
                    <li><a href="/cart">Cart</a></li>
                    <li><a href="/booking">Back</a></li>
                </ul>
              </div>
            </div>
          </nav>


        <div class="myclass">
            <form action="/pricefilter" method="post">
              <div class="input-group mb-3">
                <input type="number" name="price" class="form-control" placeholder="Enter Price (4000,2000) " aria-label="Recipient's username" aria-describedby="basic-addon2">
                <div class="input-group-append">
                  <button class="btn btn-outline-secondary" type="submit">Search</button>
                </div>
              </div>
            </form>

            <br>
             <% for (var i =0; i < pricefilter.length;  i++ ) { %>
                <div class="col-sm-4">
                <div class="card" style="width: 18rem;">
                  <img src="<%= pricefilter[i].image %>" class="card-img-top" alt="...">
                  <div class="card-body">
                    <h5 class="card-title">Name: <%=pricefilter[i].name%></h5>
                    <p class="card-text">Email: <%=pricefilter[i].email%></p>
                    <p class="card-text">Phone: <%=pricefilter[i].phone%></p>
                    <p class="card-text">Address: <%=pricefilter[i].address%></p>
                    <p class="card-text">City: <%=pricefilter[i].city%></p>
                    <p class="card-text">State: <%=pricefilter[i].state%></p>
                    <p class="card-text">Stay: <%=pricefilter[i].stay%> days </p>
                    <p class="card-text">Price: <%=pricefilter[i].price%> Rs </p>
                    <a href="/success" class="btn btn-primary">Book Me </a>
                  </div>
                  <hr>
                  </div>
                </div>
                <% } %> 

        </div>

        </body>

        </html>


        
      